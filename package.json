{
  "name": "pwa-portfolio",
  "version": "1.0.0",
  "description": "My PWA Portfolio",
  "author": "pav<pavegy@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.ts --watch server",
    "build": "nuxt-ts build",
    "start": "cross-env NODE_ENV=production node server/index.ts",
    "generate": "nuxt-ts generate",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.ts,.vue --ignore-path .gitignore .",
    "deploy": "yarn run build && firebase deploy",
    "test": "jest --passWithNoTests"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue",
      "ts",
      "tsx"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "collectCoverage": true
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.1.3",
    "@nuxtjs/axios": "^5.6.0",
    "@nuxtjs/google-analytics": "^2.2.0",
    "@nuxtjs/pwa": "^3.0.0-beta.16",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "lint": "^0.7.0",
    "nuxt": "^2.9.2",
    "nuxt-property-decorator": "^2.3.0",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.2.2",
    "vuex": "^3.1.1",
    "vuex-class": "^0.3.2",
    "vuex-module-decorators": "^0.10.1"
  },
  "devDependencies": {
    "@nuxt/typescript": "^2.8.1",
    "@nuxt/typescript-build": "^0.1.11",
    "@nuxtjs/eslint-config": "^1.1.2",
    "@nuxtjs/vuetify": "^1.6.1",
    "@types/jest": "^24.0.18",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.1.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-nuxt": "^0.4.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-plugin-vue": "^5.2.3",
    "firebase-tools": "^7.3.0",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "nuxt-sass-resources-loader": "^2.0.5",
    "prettier": "^1.18.2",
    "sass-loader": "^7.3.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript-eslint-parser": "^22.0.0",
    "vue-eslint-parser": "^6.0.4",
    "vue-jest": "^3.0.4",
    "vue-test-utils": "^1.0.0-beta.11"
  }
}
